<% provide(:title, "拠点情報編集てすと") %>

<h1>拠点情報の編集てすと</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@base, url: update_test01_path(params: {id: @base.id})) do |f| %>
      <!-- %= render 'shared/error_messages', object: @base %>-->

      <%= f.label :base_number %>
      <%= f.text_field :base_number, class: 'form-control' %>

      <%= f.label :base_name %>
      <%= f.text_field :base_name, class: 'form-control' %>

      <%= f.submit "更新する", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>